        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:label="@string/main_Button_2" android:name=".Recieve_server"></activity>
        <activity android:label="@string/main_Button_1" android:name=".Send_Server" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>